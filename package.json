  "description": "es6 Node RESTful API Boilerplate",
  "main": "app",
  "repository": {},
  "author": "Josh English <josh@joshenglish.com>",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src --presets es2015,es2016\"",
